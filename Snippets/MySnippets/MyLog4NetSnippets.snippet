<?xml version="1.0" encoding="utf-8"?>  
<!--NOTE: be sure to make this file's extension a .snippet, otherwise VS won't recognize it as a snippet!-->
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">  
    <CodeSnippet Format="1.0.0">  
        <Header>			
            <Title>MyLog4NetDebug</Title>          
			<Shortcut>logDebug</Shortcut>			
			<Description></Description>
			<HelpUrl></HelpUrl>
		</Header>  
        <Snippet> 	
		<Declarations>  					
			<Literal>  
				<ID>MyText</ID>  
				<ToolTip></ToolTip>  
				<Default>TextHere</Default>  
			</Literal>  
		</Declarations>  					
			<Code Language="CSharp" >  
                <![CDATA[_logger.LogDebug(string.Format("$MyText$ {0}: {1}", MethodBase.GetCurrentMethod().Name, ex.ToString()));$end$]]>  
			</Code>  
        </Snippet>  
    </CodeSnippet>  
	
	<CodeSnippet Format="1.0.0">  
        <Header>			
