			<Description></Description>
			<HelpUrl></HelpUrl>
		</Header>  
        <Snippet> 						
			<Code Language="CSharp" >  
                <![CDATA[_logger.LogInfo("$end$");]]>  
			</Code>  
        </Snippet>  
    </CodeSnippet>  
	<CodeSnippet Format="1.0.0">  
        <Header>			
            <Title>MyLog4NetException</Title>          
