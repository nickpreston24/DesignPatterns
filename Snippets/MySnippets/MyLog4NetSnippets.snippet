			<Description></Description>
			<HelpUrl></HelpUrl>
		</Header>  
        <Snippet> 						
			<Code Language="CSharp" >  
                <![CDATA[_logger.LogError(ex);]]>  
			</Code>  
        </Snippet>  
    </CodeSnippet>  
	<CodeSnippet Format="1.0.0">  
        <Header>			
            <Title>MyLog4NetInstance</Title>          
			<Shortcut>logger</Shortcut>			
			<Description></Description>
			<HelpUrl></HelpUrl>
		</Header>  
        <Snippet> 						
			<Code Language="CSharp" >  
                <![CDATA[private static IClientLogger _logger = new ClientLogger(); $end$]]>  
			</Code>  
        </Snippet>  
    </CodeSnippet>  
	
</CodeSnippets>  