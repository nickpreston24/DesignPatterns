    <CodeSnippet Format="1.0.0">  
        <Header>			
            <Title>MyDebugWriteLine_w_Exception</Title>          
			<Shortcut>dwe</Shortcut>			
			<Description></Description>
			<HelpUrl></HelpUrl>
		</Header>  
        <Snippet> 					
			<Imports>
				<Import>
					<Namespace>System.Reflection</Namespace>
				</Import>
				<Import>
					<Namespace>System.Diagnostics</Namespace>
				</Import>
				<Import>
					<Namespace>System</Namespace>
				</Import>
			</Imports>		
			<Code Language="CSharp" >  
                <![CDATA[Debug.WriteLine(string.Format("{0}: {1}", MethodBase.GetCurrentMethod().Name, ex.ToString()));$end$]]>  
			</Code>  
        </Snippet>  
    </CodeSnippet>  
	<CodeSnippet Format="1.0.0">  
        <Header>  
            <Title>MyDebugWriteLine</Title>          
