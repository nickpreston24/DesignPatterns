    </CodeSnippet>  
	
	<CodeSnippet Format="1.0.0">  
        <Header>			
            <Title>title</Title>          
			<Shortcut>dwv</Shortcut>			
			<Description></Description>
			<HelpUrl></HelpUrl>
		</Header>  
        <Snippet> 		
			<Imports>
				<Import>
					<Namespace>System.Reflection</Namespace>
				</Import>
				<Import>
					<Namespace>System.Diagnostics</Namespace>
				</Import>
				<Import>
					<Namespace>System</Namespace>
				</Import>
			</Imports>	
			<Declarations>  
			<Literal>  
				<ID>MyVariableName</ID>  
				<ToolTip>Replace with Variable name.</ToolTip>  
				<Default>MyVariableName</Default>  
			</Literal>  			
		</Declarations>  		
			<Imports>
				<Import>
					<Namespace></Namespace>
				</Import>
			</Imports>					
			<Code Language="CSharp" >  
                <![CDATA[				
				Debug.WriteLine(string.Format("{0}() - $MyVariableName$: {1}", MethodBase.GetCurrentMethod().Name,  $MyVariableName$));
				$end$ 
				]]>  
			</Code>  
        </Snippet>  
    </CodeSnippet>  
</CodeSnippets>  