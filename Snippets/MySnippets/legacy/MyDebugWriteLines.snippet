<?xml version="1.0" encoding="utf-8"?>
<!--NOTE: be sure to make this file's extension a .snippet, otherwise VS won't recognize it as a snippet!-->
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>MyDebugWriteLine_w_Exception</Title>
      <Shortcut>dwe</Shortcut>
      <Description></Description>
      <HelpUrl></HelpUrl>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System.Diagnostics</Namespace>
        </Import>
      </Imports>
      <Code Language="CSharp" >
        <![CDATA[Debug.WriteLine(string.Format("{0}: {1}", MethodBase.GetCurrentMethod().Name, ex.ToString()));$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>MyDebugWriteLine</Title>
      <Shortcut>dw</Shortcut>
      <Type>Extension</Type>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System.Diagnostics</Namespace>
        </Import>
      </Imports>
      <Code Language="CSharp">
        <![CDATA[
				Debug.WriteLine($selected$$end$);
				]]>
      </Code>
    </Snippet>
  </CodeSnippet>

  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>title</Title>
      <Shortcut>dwv</Shortcut>
      <Description></Description>
      <HelpUrl></HelpUrl>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>MyVariableName</ID>
          <ToolTip>Replace with Variable name.</ToolTip>
          <Default>MyVariableName</Default>
        </Literal>
      </Declarations>
      <Imports>
        <Import>
          <Namespace>System.Diagnostics</Namespace>
        </Import>
      </Imports>
      <Code Language="CSharp" >
        <![CDATA[				
				Debug.WriteLine(string.Format("{0}() - $MyVariableName$: {1}", MethodBase.GetCurrentMethod().Name,  $MyVariableName$));
				$end$ 
				]]>
      </Code>
    </Snippet>

  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>MyDebugMessageDefault</Title>
      <Author>mikenp</Author>
      <Description>
      </Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>
        dwe6
      </Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System.Diagnostics</Namespace>
        </Import>
      </Imports>
      <Code Language="csharp" Delimiter="$"><![CDATA[Debug.WriteLine($$"{MethodBase.GetCurrentMethod().Name}: {ex.ToString()}");$end$ ]]></Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>MyDebugMessage_cs6</Title>
      <Author>mikenp</Author>
      <Description>
        This custom Debug message uses Interpolation that came from C# 6.0
      </Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>
        MyDebugMessage_cs6
      </Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>MyVar</ID>
          <ToolTip>Enter the Variable Name</ToolTip>
          <Default>MyVar</Default>
        </Literal>
      </Declarations>
      <Imports>
        <Import>
          <Namespace>System.Diagnostics</Namespace>
        </Import>
      </Imports>
      <Code Language="csharp" Delimiter="$">
        <![CDATA[
	  Debug.WriteLine($$"'{MethodBase.GetCurrentMethod().Name}()': {nameof($MyVar$)} = {$MyVar$}");	  
	  ]]>
      </Code>
    </Snippet>
  </CodeSnippet>

  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>UnitTestDebug</Title>
      <Author>mikenp</Author>
      <Description>
      </Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>
        Debug-UnitTests
      </Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>MyVar</ID>
          <ToolTip>Enter the Variable Name</ToolTip>
          <Default>MyVar</Default>
        </Literal>
      </Declarations>
      <Imports>
        <Import>
          <Namespace>System.Diagnostics</Namespace>
        </Import>
      </Imports>
      <Code Language="csharp" Delimiter="$">
        <![CDATA[
	  Debug.WriteLine("Expected: " + expected, "Actual: " + actual);
	  ]]>
      </Code>
    </Snippet>
  </CodeSnippet>


</CodeSnippets>
